{
  "name": "react-dancing",
  "version": "0.2.4",
  "description": "react-dancing is a bit module for animation",
  "main": "index.cjs",
  "type": "module",
  "module": "index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./index.cjs",
      "import": "./index.js"
    }
  },
  "types": "react-dancing.d.ts",
  "scripts": {
    "test": "npm run test",
    "build-esmodule":"cp src/index.js ./index.js",
    "build-commonjs": "babel src/index.js --out-file ./index.cjs",
    "build":"npm run build-commonjs && npm run build-esmodule",
    "lint": "eslint ./src",
    "format": "prettier ./src --write --ignore-unknown",
    "super-minify": "node scripts/super-minify.js",
    "preversion": "npm run format && npm run lint",
    "version": "npm run build && npm run super-minify",
    "postversion": "git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/behnammodi/react-dancing.git"
  },
  "keywords": [
    "animation"
  ],
  "author": "Behnam Mohammadi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/behnammodi/react-dancing/issues"
  },
  "homepage": "https://github.com/behnammodi/react-dancing#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "babel-preset-minify": "^0.5.1",
    "eslint": "^7.7.0",
    "eslint-plugin-react": "^7.20.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.13",
    "prettier": "2.1.1"
  },
  "peerDependencies": {
    "react": "^16.13.1"
  }
}
